{
	"_version": "1.5.0",
	"sap.app": {
		"id": "demo.odata",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"mainService": {
				"uri": "/oData_Demos/V2/OData/OData.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "demo.odata.view.S0",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "mainService",
				"preload": true
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"preload": false,
				"async": true,
				"uri": "i18n/i18n.properties"
			}
		},
		"resources": {
			"css": []
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "demo.odata.view",
				"controlId": "root",
				"controlAggregation": "masterPages",
				"clearAggregation": true,
				"viewType": "XML",
				"bypassed": {
					"target": ["categories"]
				}
			},
			"targets": {
				"categories": {
					"viewName": "S1"
				},
				"products": {
					"viewName": "S2",
					"controlAggregation": "detailPages"
				}
			},
			"routes": [{
				"name": "default",
				"pattern": "",
				"target": ["categories"]
			}, {
				"name": "products",
				"pattern": "category/{category_id}",
				"target": ["products", "categories"]
			}]
		}
	}
}